{
  "$schema": "https://json.schemastore.org/expo-53.0.0.json",
  "expo": {
    "name": "Luna clone",
    "slug": "luna-clone",
    "description": "Expo based clone of the Luna app",
    "version": "0.0.0",
    "platforms": ["android"],
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.brutcha.lunaclone",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "experiments": {
      "reactCompiler": true
    }
  }
}
